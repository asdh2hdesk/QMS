<odoo>
    <record id="ir_cron_auto_generate_calibrations" model="ir.cron">
        <field name="name">Auto Generate Calibration Schedules</field>
        <field name="model_id" ref="model_calibration_schedule"/>
        <field name="state">code</field>
        <field name="code">model.generate_future_calibrations()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
